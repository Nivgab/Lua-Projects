local function LCSJ_fake_script() -- FakeCrash_2.showCrash 
	local script = Instance.new('LocalScript', FakeCrash_2)

	-- Decompiled with the Synapse X Luau decompiler.

	local l__TweenService__1 = game:GetService("TweenService");
	local l__UserInputService__2 = game:GetService("UserInputService");
	local l__RunService__3 = game:GetService("RunService");
	local l__mouse__4 = game.Players.LocalPlayer:GetMouse();
	require(script:WaitForChild("DraggableObject")).new(script.Parent.CrashMessage):Enable();
	l__RunService__3.RenderStepped:Connect(function()
		if script.Parent.Visible and script.Parent.Parent.Enabled then
			l__UserInputService__2.MouseBehavior = Enum.MouseBehavior.Default;
			l__UserInputService__2.MouseIconEnabled = false;
			script.Parent.FakeMouse.Position = UDim2.new(0, l__mouse__4.X, 0, l__mouse__4.Y + 72);
		end;
	end);
	script.Parent:GetPropertyChangedSignal("Visible"):Connect(function()
		if not script.Parent.Visible or not script.Parent.Parent.Enabled then
			l__UserInputService__2.MouseIconEnabled = true;
			return;
		end;
		l__UserInputService__2.MouseIconEnabled = false;
		script.Parent.CrashMessage.Position = UDim2.new(0.5, 0, 0.5, 0);
